<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:padding="@dimen/default_normal"
    android:background="@drawable/selector_default_primary">
    <ImageView
        android:id="@+id/list_item_channel_icon"
        android:layout_width="@dimen/default_larger"
        android:layout_height="@dimen/default_larger"
        app:srcCompat="@drawable/ic_guitar_mini"
        android:layout_marginRight="@dimen/default_normal"
        android:visibility="gone"/>
    <TextView
        android:id="@+id/list_item_channel_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/default_large"
        android:layout_marginRight="@dimen/default_normal"
        style="@style/Widget.Text.SubTitle.Invert"
        tools:text="Title"/>
</FrameLayout>